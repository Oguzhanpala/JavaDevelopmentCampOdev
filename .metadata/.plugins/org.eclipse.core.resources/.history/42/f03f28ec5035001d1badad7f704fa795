package loopDemo;

public class main {

	public static void main(String[] args) {
		//for döngüsü
		for(int i=1; i<10; i++) {
			System.out.println(i);
		}
		System.out.println("döngü bitti");
		
		// tek sayılar
		for(int i=1; i<10; i=i+2) {
			System.out.println(i);
		}
		
		//çift sayılar
		for(int i=0; i<10; i=i+2) {
			System.out.println(i);
		}


	}

}
